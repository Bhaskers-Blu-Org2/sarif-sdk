{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/sarif-2.1.0-rtm.5.json",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeScanner",
          "rules": [
            {
              "id": "TST0001",
              "helpUri": "http://www.example.com/rules/tst0001.html",
              "shortDescription": {
                "text": "Some testing occurred."
              },
              "fullDescription": {
                "text": "Some testing occurred. Full text."
              },
              "messageStrings": {
                "Default": {
                  "text": "Some testing occurred."
                }
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "TST0001",
          "level": "error",
          "message": {
            "id": "Default"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "file:///c:/src/file.c"
                },
                "region": {
                  "startLine": 1,
                  "startColumn": 5,
                  "endLine": 1,
                  "endColumn": 5
                }
              },
              "message": {
                "text": "endColumn equals startColumn."
              }
            },
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "file:///c:/src/file.c"
                },
                "region": {
                  "startLine": 1,
                  "startColumn": 5,
                  "endColumn": 5
                }
              },
              "message": {
                "text": "endColumn equals startColumn; endLine not specified."
              }
            },
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "file:///c:/src/file.c"
                },
                "region": {
                  "startLine": 1,
                  "startColumn": 1,
                  "endColumn": 2
                }
              },
              "message": {
                "text": "endColumn greater than startColumn."
              }
            },
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "file:///c:/src/file.c"
                },
                "region": {
                  "startLine": 1,
                  "startColumn": 2
                }
              },
              "message": {
                "text": "endColumn not specified."
              }
            }
          ]
        }
      ],
      "columnKind": "utf16CodeUnits"
    }
  ]
}